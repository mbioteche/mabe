{
	"name": "mabe-website",
	"version": "1.2.0",
	"private": true,
	"scripts": {
		"build": "next build",
		"build:analyze": "cross-env ANALYZE=true npm run build",
		"clean": "git clean --force -dx --exclude .env.local --exclude .env.*.local --exclude node_modules/ --exclude .husky/",
		"dev": "next dev",
		"fix": "run-s fix:*",
		"fix:eslint": "npm run lint:eslint -- --fix",
		"fix:prettier": "npm run lint:prettier -- --write",
		"lint": "run-p lint:*",
		"lint:eslint": "eslint --ext .js,.ts,.tsx --ignore-path ./.gitignore .",
		"lint:prettier": "prettier --check --ignore-path ./.gitignore .",
		"lint:typescript": "tsc",
		"prepare": "husky install",
		"start": "next start",
		"cms-typegen": "contentful-typescript-codegen --output src/@types/generated/contentful.d.ts",
		"changeset": "changeset",
		"ctg": "cf-content-types-generator"
	},
	"lint-staged": {
		"*": "prettier --write --ignore-unknown",
		"*.{js,ts,tsx}": "eslint --fix"
	},
	"dependencies": {
		"@contentful/rich-text-react-renderer": "^15.19.4",
		"@contentful/rich-text-types": "^16.3.4",
		"autoprefixer": "^10.4.17",
		"babel-plugin-optimize-clsx": "^2.6.2",
		"clsx": "^2.1.0",
		"contentful": "^10.6.21",
		"contentful-management": "^11.15.0",
		"next": "^14.1.0",
		"next-compose-plugins": "^2.2.1",
		"postcss-focus-visible": "^9.0.1",
		"postcss-import": "^16.0.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-icons": "^5.0.1",
		"sharp": "^0.33.2",
		"tailwindcss": "^3.4.1"
	},
	"devDependencies": {
		"@changesets/cli": "^2.27.1",
		"@tailwindcss/typography": "^0.5.10",
		"@tsconfig/next": "^2.0.1",
		"@types/node": "^20.11.17",
		"@types/react": "^18.2.55",
		"@typescript-eslint/eslint-plugin": "^6.21.0",
		"@typescript-eslint/parser": "^6.21.0",
		"cf-content-types-generator": "^2.14.0",
		"cross-env": "^7.0.3",
		"dotenv": "^16.4.2",
		"eslint": "^8.56.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-airbnb-typescript": "^17.1.0",
		"eslint-config-next": "^14.1.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"eslint-plugin-etc": "^2.0.3",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-jsx-a11y": "^6.8.0",
		"eslint-plugin-react": "^7.33.2",
		"eslint-plugin-react-etc": "^2.0.1",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-simple-import-sort": "^12.0.0",
		"eslint-plugin-tailwindcss": "^3.14.2",
		"husky": "^9.0.10",
		"lint-staged": "^15.2.2",
		"npm-run-all": "^4.1.5",
		"prettier": "^3.2.5",
		"typescript": "^5.3.3"
	}
}
